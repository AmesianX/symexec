echo -e "#############$TARGET TESTING END################\n"
TARGET=last_fermat_4.mem2reg.instnamer
echo "#############$TARGET TESTING BEGIN################"
./run.sh ${TARGET} 2>&1 | diff -s - oracles/${TARGET}.oracle
echo -e "#############$TARGET TESTING END################\n" 
TARGET=common_subexpression
echo "#############$TARGET TESTING BEGIN################"
./run.sh ${TARGET} 2>&1 | diff -s - oracles/${TARGET}.oracle
echo -e "#############$TARGET TESTING END################\n" 


